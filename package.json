{
  "name": "gwt-runner",
  "version": "2.1.0",
  "description": "A small library to help Jest support given-when-then style testing without a bunch of overhead",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib",
    "src",
    "!**/*.spec.ts",
    "!**/*.spec.d.ts",
    "!**/*.spec.js.map",
    "!**/*.spec.js"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --project ./tsconfig.json",
    "test": "jest",
    "lint": "eslint ./src ./examples",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "run-s clean build test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devzeebo/gwt-runner.git"
  },
  "keywords": [
    "gwt",
    "unit",
    "testing",
    "bdd",
    "given",
    "when",
    "then"
  ],
  "author": "Eric Siebeneich <eric.siebeneich@gmail.com> (https://github.com/devzeebo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devzeebo/gwt-runner/issues"
  },
  "homepage": "https://github.com/devzeebo/gwt-runner#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^28.1.3",
    "@react-ddd/eslint-config": "^0.2.1",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "axios": "^0.27.2",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-watch": "^8.0.0",
    "jest": "^28.1.3",
    "jest-gwt": "^3.0.3",
    "jest-mock": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  }
}