{
  "name": "gwt-runner",
  "version": "2.2.0",
  "description": "A small library to help Jest support given-when-then style testing without a bunch of overhead",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "types",
    "!**/*.spec.ts"
  ],
  "scripts": {
    "build": "wireit",
    "test": "wireit",
    "lint": "wireit",
    "test:coverage": "wireit",
    "prepublishOnly": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --project ./tsconfig.build.json",
      "files": [
        "src/**/*.ts",
        "types/**/*.d.ts",
        "tsconfig.*",
        "!src/**/*.spec.ts"
      ],
      "output": [
        "lib/**"
      ]
    },
    "lint": {
      "command": "eslint ./src ./examples"
    },
    "test": {
      "command": "jest"
    },
    "test:coverage": {
      "command": "jest --coverage"
    },
    "prepublishOnly": {
      "dependencies": [
        "lint",
        "test:coverage",
        "build"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devzeebo/gwt-runner.git"
  },
  "keywords": [
    "gwt",
    "unit",
    "testing",
    "bdd",
    "given",
    "when",
    "then"
  ],
  "author": "Eric Siebeneich <eric.siebeneich@gmail.com> (https://github.com/devzeebo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devzeebo/gwt-runner/issues"
  },
  "homepage": "https://github.com/devzeebo/gwt-runner#readme",
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@react-ddd/eslint-config": "^0.2.1",
    "@swc/core": "^1.2.242",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "axios": "^0.27.2",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "jest-mock": "^28.1.3",
    "typescript": "^4.7.4",
    "wireit": "^0.7.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  }
}