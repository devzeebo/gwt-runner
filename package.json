{
  "name": "gwt-runner",
  "version": "1.2.0",
  "description": "A small library to help Jest support given-when-then style testing without a bunch of overhead",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "!**/*.spec.js"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run build && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devzeebo/gwt-runner.git"
  },
  "keywords": [
    "gwt",
    "unit",
    "testing",
    "bdd",
    "given",
    "when",
    "then"
  ],
  "author": "Eric Siebeneich <eric.siebeneich@gmail.com> (https://github.com/devzeebo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devzeebo/gwt-runner/issues"
  },
  "homepage": "https://github.com/devzeebo/gwt-runner#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@jest/globals": "^27.0.6",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-watch": "^7.0.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
